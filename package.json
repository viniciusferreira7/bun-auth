{
	"name": "bun-auth",
	"version": "1.0.50",
	"scripts": {
		"db:generate": "bun --env-file .env --bun drizzle-kit generate",
		"db:migrate": "bun --env-file .env --bun  drizzle-kit migrate",
		"db:push": "bun --env-file .env drizzle-kit push",
		"db:studio": "bunx --bun drizzle-kit studio --port 3000 --verbose",
		"predev": "docker compose --env-file .env up -d bun-auth-postgresql",
		"dev": "bun run --watch src/index.ts",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check": "biome check --write"
	},
	"dependencies": {
		"@elysiajs/openapi": "^1.4.10",
		"better-auth": "^1.3.23",
		"drizzle-orm": "^0.44.5",
		"elysia": "latest",
		"pg": "^8.16.3",
		"postgres": "^3.4.7",
		"zod": "^4.1.11"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@types/pg": "^8.15.5",
		"bun-types": "latest",
		"drizzle-kit": "^0.31.5"
	},
	"module": "src/index.js"
}
